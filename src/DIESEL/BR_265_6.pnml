							0: spriteset_BR_265_6;
							1: return switch_animation_BR_265_6;
							2: spriteset_BR_265_6_lights_front;}	
							
						switch(FEAT_TRAINS, SELF, switch_spritestack_BR_265_6_REV, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 2 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
