							1: switch_spritestack_BR_23_DB_Schwarz;
							2: switch_spritestack_BR_23_DB_Reifen;							
								switch_spritestack_BR_23_DB_Silber;}	
								
						switch (FEAT_TRAINS, SELF, random_choose_BR_23_DB_Tender, switch_alternative_var_random_bits(1, 2)) {
							0: spriteset_BR_23_DB_Tender_B;
							1: spriteset_BR_23_DB_Tender_A;
							2: spriteset_BR_23_DB_Tender_A;							
								spriteset_BR_23_DB_Tender_B;}								

						switch(FEAT_TRAINS, SELF, switch_graphics_BR_23_DB, position_in_articulated_veh % 4) {
							0:	   spriteset_empty;
							1:     return random_choose_BR_23_DB;
							2:	   spriteset_empty;
							3:     return random_choose_BR_23_DB_Tender;}																																											

						switch(FEAT_TRAINS, SELF, switch_length_BR_23_DB, position_in_vehid_chain % 4) {
							0: 3;
							1: 3;
							2: 3;
							3: 5;}

						switch(FEAT_TRAINS, SELF, switch_FX_BR_23_DB, position_in_vehid_chain % 4) {
							0: return visual_effect_and_powered(VISUAL_EFFECT_STEAM, -2, DISABLE_WAGON_POWER);
							return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);}	

						switch (FEAT_TRAINS, SELF, switch_articulated_BR_23_DB, extra_callback_info1) {
