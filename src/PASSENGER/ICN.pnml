			cost_factor						: 750;
			running_cost_factor				: switch_running_cost_ICN_half;
			}
		}

//Switches for Short Set

						switch(FEAT_TRAINS, SELF, switch_ICN_Length_short, position_in_vehid_chain % 11) {
							0 .. 10: 7;}	
							
						switch(FEAT_TRAINS,PARENT,switch_running_cost_ICN_short, 1){
							return 3600;}																																										

						switch(FEAT_TRAINS, SELF, switch_ICN_short_blue_graphics, position_in_vehid_chain % 11) {
							0: spriteset_ICN_front_blue;
