							2: switch_spritestack_ICN_middle_B_blue;
							3: spriteset_ICN_middle_C_blue;
							4: switch_spritestack_ICN_middle_D_blue;
							5: spriteset_ICN_middle_C_blue;
							6: spriteset_ICN_middle_E_blue;
							7: switch_spritestack_ICN_middle_F_blue;
							8: switch_spritestack_ICN_middle_G_blue;
							9: switch_spritestack_ICN_middle_H_blue;
							10: spriteset_ICN_back_blue;}
							
						switch (FEAT_TRAINS, SELF, switch_articulated_ICN_short_blue, extra_callback_info1) {
							1 .. 10: return ITEM_ICN_SHORT_BLUE; 
							return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}							

