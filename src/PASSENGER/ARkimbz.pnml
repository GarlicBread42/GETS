//Switches

						switch (FEAT_TRAINS,SELF,switch_purchase_ARkimbz,current_year) {
							   0 .. 1999 : spriteset_ARkimbz_Interregio_purchase;
							2000 .. 2050 : spriteset_ARkimbz_IC_purchase;
								spriteset_ARkimbz_IC_purchase;}	
							
