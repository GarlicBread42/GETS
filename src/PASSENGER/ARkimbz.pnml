							1: spritegroup_ARkimbz_loading;}
							   																			
						switch(FEAT_TRAINS, SELF, switch_length_ARkimbz, position_in_vehid_chain % 3) {
							0: 4;
							1: 7;
							2: 4;}
						
						switch(FEAT_TRAINS,PARENT,switch_running_cost_ARkimbz, 1){
							return 7800;}	
							
						switch (FEAT_TRAINS,SELF,switch_build_year_ARkimbz,build_year) {
							   0 .. 1999 : switch_spritestack_ARkimbz_Interregio;
							2000 .. 2050 : switch_spritestack_ARkimbz_IC;
								switch_spritestack_ARkimbz_IC;}													   								

						switch(FEAT_TRAINS, SELF, switch_ARkimbz_graphics_build, position_in_vehid_chain  % 3) {
							0:     spriteset_empty;
							1:     return switch_build_year_ARkimbz;
							2:     spriteset_empty;}	

						switch (FEAT_TRAINS, SELF, switch_articulated_ARkimbz_build, extra_callback_info1) {
							1 ..2: return ITEM_ARMKIMBZ_BUILD;
								return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}														

