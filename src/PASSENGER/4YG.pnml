//Switches

						switch(FEAT_TRAINS, SELF, switch_loadstack_4YG_DB_AB, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_4YG_DB_AB;
							1: spritegroup_4YG_loading_DB;}
							
						switch(FEAT_TRAINS, SELF, switch_loadstack_4YG_DB_B, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_4YG_DB_B;
							1: spritegroup_4YG_loading_DB;}							

						switch (FEAT_TRAINS, SELF, random_choose_4YG_DB, switch_alternative_var_random_bits(1, 1)) {
							1: switch_loadstack_4YG_DB_AB;
								switch_loadstack_4YG_DB_B;}	
								
						switch(FEAT_TRAINS, SELF, switch_loadstack_4YG_AZB_AB, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_4YG_AZB_AB;
							1: spritegroup_4YG_loading_DB;}
							
						switch(FEAT_TRAINS, SELF, switch_loadstack_4YG_AZB_B, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_4YG_AZB_B;
							1: spritegroup_4YG_loading_DB;}							

						switch (FEAT_TRAINS, SELF, random_choose_4YG_AZB, switch_alternative_var_random_bits(1, 1)) {
							1: switch_loadstack_4YG_AZB_AB;
								switch_loadstack_4YG_AZB_B;}									
							
						switch(FEAT_TRAINS, SELF, switch_4YG_graphics, position_in_articulated_veh  % 3) {
							0:     spriteset_empty;
							1:     return random_choose_4YG_DB;
							2:     spriteset_empty;}									
					
						switch(FEAT_TRAINS, SELF, switch_length_4yg_DB, position_in_articulated_veh % 3) {
							0: 4;
							1: 4;
							2: 4;}

						switch (FEAT_TRAINS, SELF, switch_articulated_4YG_DB, extra_callback_info1) {
							1: return ITEM_4YG_DB;
							2: return ITEM_4YG_DB;
								return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}
						
						switch(FEAT_TRAINS,PARENT,switch_running_cost_4YG, 1){
								return 1750;}						


